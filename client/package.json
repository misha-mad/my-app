{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "download-schema": "gq http://localhost:8080/v1/graphql -H 'X-Hasura-Admin-Secret:111' --introspect > schema.graphql",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "3.7.17",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.22.3",
    "graphql": "16.7.1",
    "graphql-request": "6.1.0",
    "graphql-ws": "5.14.0",
    "svelte": "4.1.1",
    "svelte-check": "3.4.6",
    "vite": "4.4.6",
    "vitest": "0.33.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "@ottofeller/prettier-config-ofmt": "1.7.2",
    "@playwright/test": "1.36.1",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte": "2.32.2",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6"

  },
  "prettier": "@ottofeller/prettier-config-ofmt"
}
